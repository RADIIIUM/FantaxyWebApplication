}
<meta charset="utf-8">
<link rel="stylesheet" href="~/css/main.css" asp-append-version="true" />
<link rel="stylesheet" href="~/css/items.css" asp-append-version="true" />

<div class="div-link">
    <a asp-controller="Planet" asp-action="CreatePlanet">
        <span class="material-symbols-outlined" data-tooltip="Создать свою планету">
            public
        </span>
